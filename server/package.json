{
  "name": "flashcards-backend",
  "version": "0.0.1",
  "description": "The backend for a basic flashcards app",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "migrate": "DATABASE_URL=postgres://postgres:@localhost:5432/flashcards node-pg-migrate"
  },
  "repository": "https://github.com/jhonnold/flashcards",
  "author": "Jay Honnold",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "morgan": "^1.8.0",
    "node-pg-migrate": "^3.1.0",
    "pg": "^7.4.1",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "nodemon": "^1.9.2"
  }
}
